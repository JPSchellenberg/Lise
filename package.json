{
  "name": "lise",
  "version": "1.0.0-prerelease.9",
  "description": "Low-cost interface for science experiments",
  "main": "./app/electron.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/JPSchellenberg/Lise"
  },
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --watch --watch-extensions tsx,ts,test.ts --require ts-node/register --recursive ./**/*.test.ts",
    "clear": "rm -rf dist && rm -rf build",
    "uninstall": "npm run clear && rm -rf node_modules && rm -rf typings",
    "dev": "NODE_ENV=development VERSION=$(git describe --dirty) ./node_modules/.bin/webpack-dev-server --port 8080 --config './webpack.js' --progress --colors",
    "build": "npm run build:server && npm run build:client",
    "dist": "npm run build:server:production && npm run build:client",
    "dist:openwrt": "npm run clear && npm run dist && mkdir -p dist/resources && cp -r resources/openWRT/ dist/resources && mv dist/resources/install dist/ && mkdir -p dist/sketch && cp sketch/lise_yun/lise_yun.ino dist/sketch/sketch.ino && cp docs/Installation/openWRT.md dist/README.md && mv dist/resources/lib/node_modules/serialport dist/server/node_modules/serialport",
    "build:server": "./node_modules/.bin/tsc server/index.ts --sourceMap --inlineSources --module commonjs --target es5 --outDir build/server",
    "build:server:test": "NODE_ENV=production ./node_modules/.bin/webpack -p --config './webpack.server.js' --progress --colors",
    "build:server:production": "NODE_ENV=production ./node_modules/.bin/tsc/ server/index.ts --module commonjs --target es5 --outDir dist/server && cp server/package.json dist/server && cd dist/server; npm install",
    "build:client": "NODE_ENV=production VERSION=$(git describe) ./node_modules/.bin/webpack -p --config './webpack.js' --progress --colors",
    "build:mac": "npm run build && cp electron/* build/ && mv build/ app/ && ./node_modules/.bin/build -m",
    "build:win": "npm run build && cp electron/* build/ && mv build/ app/ && ./node_modules/.bin/build --win --ia32",
    "postinstall": "./node_modules/.bin/typings install"
  },
  "author": "Jan Philip Schellenberg",
  "license": "",
  "devDependencies": {
    "chai": "3.5.0",
    "concurrently": "3.1.0",
    "css-loader": "0.25.0",
    "electron-builder": "8.6.0",
    "electron-prebuilt": "1.4.6",
    "file-loader": "0.9.0",
    "jsonfile": "2.4.0",
    "mocha": "2.5.3",
    "node-sass": "3.10.1",
    "react-hot-loader": "1.3.0",
    "sass-loader": "4.0.2",
    "semver": "5.3.0",
    "source-map-loader": "0.1.5",
    "style-loader": "0.13.1",
    "ts-loader": "0.8.2",
    "ts-node": "1.2.2",
    "typescript": "1.8.9",
    "typings": "1.3.2",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "webpack-dev-server": "1.14.1"
  },
  "dependencies": {
    "avrgirl-arduino": "1.9.8",
    "body-parser": "1.15.2",
    "bootstrap": "3.3.7",
    "classnames": "2.2.5",
    "debug": "^2.3.3",
    "electron-packager": "8.2.0",
    "electron-squirrel-startup": "1.0.0",
    "eventemitter3": "2.0.2",
    "express": "4.14.0",
    "isomorphic-fetch": "2.2.1",
    "jquery": "3.1.1",
    "lodash": "4.15.0",
    "mime": "1.3.4",
    "page": "1.7.1",
    "react": "15.3.0",
    "react-bootstrap": "0.30.6",
    "react-dom": "15.3.0",
    "react-redux": "4.4.5",
    "redux": "3.5.2",
    "redux-devtools-extension": "1.0.0",
    "redux-thunk": "2.1.0",
    "serialport": "4.0.6",
    "socket.io": "1.5.1"
  },
  "build": {
    "appId": "org.lise.education",
    "win": {
      "target": "nsis",
      "iconUrl": "https://cdn0.iconfinder.com/data/icons/thin-hardware/24/thin-0332_computer_laptop_website_url_www-512.png"
    }
  }
}
